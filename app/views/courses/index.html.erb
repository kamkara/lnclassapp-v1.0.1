<p id="notice"><%= notice %></p>

<h1>Courses</h1>

<table class="container">
  <thead>
    <tr>
      <th>Content</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody >
    <% @courses.each do |course| %>
      <tr>
        <td><%= course.content %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course', new_course_path %>
